{
  "name": "create-t3-app",
  "version": "7.39.3",
  "description": "Create web application with the t3 stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/t3-oss/create-t3-app.git",
    "directory": "cli"
  },
  "keywords": [
    "create-t3-app",
    "init.tips",
    "next.js",
    "t3-stack",
    "tailwind",
    "tRPC",
    "typescript"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-t3-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".yarnrc.yml",
    "package.json"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --check",
    "release": "changeset version",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:next": "pnpm build && npm publish --tag next",
    "pub:release": "pnpm build && npm publish"
  },
  "dependencies": {
    "@clack/core": "^0.3.5",
    "@clack/prompts": "^0.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "chalk": "5.2.0",
    "commander": "^10.0.1",
    "execa": "^7.2.0",
    "fs-extra": "^11.3.0",
    "gradient-string": "^2.0.2",
    "ora": "6.3.1",
    "sort-package-json": "^2.15.1"
  },
  "devDependencies": {
    "@auth/drizzle-adapter": "^1.9.0",
    "@auth/prisma-adapter": "^1.6.0",
    "@libsql/client": "^0.14.0",
    "@planetscale/database": "^1.19.0",
    "@prisma/adapter-planetscale": "~6.6.0",
    "@prisma/client": "~6.6.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@tanstack/react-query": "^5.74.4",
    "@trpc/client": "11.0.0",
    "@trpc/next": "11.0.0",
    "@trpc/react-query": "11.0.0",
    "@trpc/server": "11.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.17.30",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "mysql2": "^3.14.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "postgres": "^3.4.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "~6.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.4",
    "tsup": "^6.7.0",
    "type-fest": "^3.13.1",
    "typescript": "^5.8.3",
    "zod": "next"
  }
}
